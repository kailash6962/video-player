  "description": "Netflix-style video streaming platform with advanced subtitle support and multi-audio tracks",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 restart video-player",
